generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id        String  @id
  email     String  @unique
  password  String
  name      String?
  projectId String  @unique
}

model ApiKeys {
  name       String
  id         String @id
  apiKeyHash String
  createdAt  String
  projectId  String
  enabled    Int
}

model Projects {
  id              String  @id
  name            String
  plan            String
  smtpDefaultFrom String?
  smtpHost        String?
  smtpPass        String?
  smtpPort        Int?
  smtpSecure      Int?
  smtpUser        String?
}
