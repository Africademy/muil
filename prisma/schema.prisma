generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id          String  @id
  email       String  @unique
  password    String
  name        String?
  projectId   String  @unique
  projectName String
}

model ApiKeys {
  name       String
  id         String @id
  apiKeyHash String
  createdAt  String
  projectId  String
  enabled    Int
}
